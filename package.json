    "electron": "^22.0.0-alpha.8",
    "electron-packager": "^17.0.0"
  },
  "dependencies": {
    "bestzip": "^2.2.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "nconf": "^0.12.0",
    "node-clipboardy": "^1.0.3",
    "node-fetch": "^2.6.11",
    "rimraf": "^5.0.1",
    "windows-shortcuts": "^0.1.6"
  }
